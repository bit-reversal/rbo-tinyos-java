    \ \ \ \ // default values of parameters

    \ \ \ \ public static final int DEFAULT_TIMEOUT= (1\<less\>\<less\>13);
    // 8192 ms

    \ \ \ \ public static final int DEFAULT_TIME_MARGIN=4; // ms

    \ \ \ \ public static final int DEFAULT_MIN_SLEEPING_TIME=
    (1\<less\>\<less\>6); // 64 ms

